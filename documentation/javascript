
// Download very large file in browser (check if fetch can be used)
url = 'https://localhost:44330/api/testdata/test/DownloadLargeFile'
fetch(url, {
  method: 'GET', // or 'PUT'
  headers: {
    'Content-Type': 'plain/text',
    'authorization': 'Bearer eyJhbGciOiJSUzI1NiIsImtpZCI6IjIzTF9NdlhLWndXUW8xeGdRZkVoWk50Z2tZZyIsIng1dCI6IjIzTF9NdlhLWndXUW8xeGdRZkVoWk50Z2tZZyIsInR5cCI6IkpXVCJ9..DnXjTazU1YbiZ5CLlYbZp44HQx_08TVCnDI_V0MtYUAnZLjBTMAYHpN78tHgBBRRan9nwjd0ovydQDETmigvC874IkdgAkdRxGH9YpCEn5-lF7pCzs_gjR1LW7WECh5GpEwZUhX4nmRaIBOYHMtw10UwMhvwZEC31Jr2E2j-fIy6Zq38KyBeyXzTTQLzQzQhrLcTnomfxVYqshlcBxZz2gePznek7nd9kzdsvhT_O3UiH2ikXPP0pSV05UdPfFIMz0X3as2RMwYJh4a9tTIwvNMLnyQ7OKxhqrnDH7NQ0nv-HGiV0SLGrFXt9470Sh97CdxZlgDPc4STc47GdamH9A'
  }
})
.then(response => response.blob())
.then(blob => {
	var url = window.URL.createObjectURL(blob);
	var a = document.createElement('a');
	a.href = url;
	a.download = "largefile.txt";
	document.body.appendChild(a); // we need to append the element to the dom -> otherwise it will not work in firefox
	a.click();
	a.remove();  //afterwards we remove the element again
});


// Print all css variables
var allCSS = [].slice.call(document.styleSheets)
  .reduce(function(prev, styleSheet) {
    if (styleSheet.cssRules) {
      return prev + [].slice.call(styleSheet.cssRules)
        .reduce(function(prev, cssRule) {
          if (cssRule.selectorText == ':root') {
            var css = cssRule.cssText.split('{');
            css = css[1].replace('}','').split(';');
            for (var i = 0; i < css.length; i++) {
              var prop = css[i].split(':');
              if (prop.length == 2 && prop[0].indexOf('--') == 1) {
                console.log(prop[0], prop[1]);
              }
            }
          }
        }, '');
    }
  }, '');

